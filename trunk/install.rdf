<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>moonshine@googlecode.com</em:id>
        <em:version>0.1.1</em:version>
        <em:type>2</em:type>

        <!-- Target application this extension can install 
        into, with minumum and maximum supported versions -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>3.0b1</em:minVersion>
                <em:maxVersion>3.0</em:maxVersion>
            </Description>
        </em:targetApplication>

        <!-- Front-end metadata -->
        <em:name>Moonshine</em:name>
        <em:description>Embedded Lua environment for Firefox</em:description>
        <em:creator>Samuel Saint-Pettersen</em:creator>
        <em:homepageURL>http://mshine.luaforge.net</em:homepageURL>
    </Description>

</RDF>
